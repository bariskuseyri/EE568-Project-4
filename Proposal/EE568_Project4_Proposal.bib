@inbook{PMMforTA,
	author = {Zhu, Z.Q.},
	publisher = {American Cancer Society},
	isbn = {9781118354179},
	title = {Permanent Magnet Machines for Traction Applications},
	booktitle = {Encyclopedia of Automotive Engineering},
	chapter = {},
	pages = {1-20},
	doi = {10.1002/9781118354179.auto045},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/9781118354179.auto045},
	eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/9781118354179.auto045},
	year = {2014},
	keywords = {axial field, brushless AC , brushless dc, constant torque, flux weakening, fractional slot, interior permanent magnet, permanent magnet, radial-field, variable flux},
	abstract = {Abstract This chapter overviews various permanent magnet (PM) brushless machines for traction applications. PM brushless DC and AC machines and drives are firstly compared in terms of constant torque and constant power capabilities, and their controllability. Various stator winding configurations, rotor topologies, and basic and novel machine topologies, including fractional slot PM, stator PM, and variable flux PM machines, as well as their features are highlighted. Then, alternate methods for enhancing the PM excitation torque and reluctance torque components, and thereby improving the torque and power capability, are described, together with methods for reducing the torque ripples and improving the overload, flux weakening, fault-tolerant, and demagnetization withstand capabilities.}
}

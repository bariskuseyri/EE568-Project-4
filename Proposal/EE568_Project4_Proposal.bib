@inbook{Pyrhonen,
publisher = {John Wiley and Sons, Ltd},
isbn = {9780470740095},
title = {Main Dimensions of a Rotating Machine},
booktitle = {Design of Rotating Electrical Machines},
chapter = {6},
pages = {281-300},
doi = {10.1002/9780470740095.ch6},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/9780470740095.ch6},
eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/9780470740095.ch6},
year = {2008},
keywords = {armature reaction, current density, field current linkage, flux density, free parameter, loadability, machine constant, main dimensions, mechanical stress, tangential stress},
abstract = {Summary In Chapter 6, the main dimensions of rotating machines and their mutual relationships are discussed from the point of view of the design procedure. Basic features of the machines, their operational characteristics, ratings, and free parameters are defined as a basis for commencing the machine design. Some empirical information related to the selection of machine parameters aiming at an optimal solution, concerning flux densities, current densities, linear current densities, tangential stress, machine constant, weight, mechanical stress, and the like, are given in tables. Mechanical, electrical, and magnetic loadability are discussed. The length of air gap is given based on the field current linkage and armature reaction comparison, as well as on some empirical expressions. Some examples are elaborated to employ the given expressions.}
}
@INPROCEEDINGS{FSDW,
  author={S. {Bandyopadhyay} and M. {van der Geest} and H. {Polinder}},
  booktitle={8th IET International Conference on Power Electronics, Machines and Drives (PEMD 2016)}, 
  title={Design optimisation of high performance fractional-slot distributed winding PM Synchronous machines for In-wheel application in Electric Vehicles}, 
  year={2016},
  volume={},
  number={},
  pages={1-7},
  abstract={In general, PM machines with conventional integer slot distributed winding (ISDW) schemes exhibit significant cogging torque resulting in high torque ripple which makes them unsuitable for EV applications which require low torque pulsations. This paper presents detailed analysis of the performance of PM Synchronous motor (PMSM) with fractional slot distributed winding (FSDW) scheme which shows significant reduction in torque ripple compared ISDW PM machines. Effect of the fractional slot winding distribution on electromagnetic torque pulsations has been analysed using Finite element (FE) computation technique. For a fair quantitative comparison between the above machine types, a flexible optimisation approach is proposed in this paper which generates Pareto fronts of the individual machine types.},
  keywords={electric vehicles;finite element analysis;machine windings;Pareto optimisation;permanent magnet machines;synchronous machines;torque motors;Pareto fronts;finite element computation;electromagnetic torque pulsations;fractional slot winding distribution;torque ripple;FSDW;fractional slot distributed winding;PM synchronous motor;cogging torque;ISDW;integer slot distributed winding;electric vehicles;in-wheel application;PM synchronous machines;high performance fractional-slot distributed winding;Electromagnetic Torque;Finite Element Method (FEM);Fractional-Slot distributed winding;Multi-Objective Optimisation;Particle Swarm Optimization;PM machines;Torque ripple},
  doi={10.1049/cp.2016.0131},
  ISSN={},
  month={April},}
